# Copy this file to .env and replace values as needed
# Example PostgreSQL connection using psycopg (psycopg3)
# SQLAlchemy dialect for psycopg: postgresql+psycopg

# Database URL format:
# postgresql+psycopg://<username>:<password>@<host>:<port>/<database>

DATABASE_URL=postgresql+psycopg://postgres:Test12345@localhost:5432/learning_platform_db

# App settings
APP_NAME=Learning Platform
DEBUG=True

# Security
SECRET_KEY=change_me_in_production

# Optional: change CORS origins (comma-separated)
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]
